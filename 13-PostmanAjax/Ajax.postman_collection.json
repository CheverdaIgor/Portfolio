  "item": [
    {
      "name": "status",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "const json = pm.response.json();\r",
              "const responseError = json.error;\r",
              "const responseCode = pm.response.code;\r",
              "if (responseCode === 404) {\r",
              "    pm.test('404 OK', () => {\r",
              "        pm.expect(responseCode).to.eql(404);\r",
              "    });\r",
              "\r",
              "    pm.test(`No Status`, () => {\r",
              "        pm.expect(responseError).to.include('No Status');\r",
              "    });\r",
              "} else {\r",
              "    const id = Math.abs(Number(\r",
              "        String(pm.variables.get('idStatus')).trim().replace(/^[\"']|[\"']$/g, '')\r",
              "    ));\r",
              "    const status = pm.variables.get('newStatus');\r",
              "    const rewriteStatus = pm.variables.get('rewriteStatus');\r",
              "    const version = Math.abs(parseFloat(\r",
              "        String(pm.variables.get('newVersion')).trim().replace(/^[\"']|[\"']$/g, '')\r",
              "    ));\r",
              "    const updateVersion = Math.abs(parseFloat(\r",
              "        String(pm.variables.get('updateVersion')).trim().replace(/^[\"']|[\"']$/g, '')\r",
              "    ));\r",
              "    const rewriteVersion = Math.abs(parseFloat(\r",
              "        String(pm.variables.get('rewriteVersion')).trim().replace(/^[\"']|[\"']$/g, '')\r",
              "    ));\r",
              "\r",
              "    const responseId = json.data.id;\r",
              "    const responseStatus = json.data.status;\r",
              "    const responseVersion = json.data.version;\r",
              "    const responseHistory = json.data.history;\r",
              "\r",
              "    pm.test('200 OK', () => {\r",
              "        pm.expect(responseCode).to.eql(200);\r",
              "    });\r",
              "\r",
              "    pm.test(`id: ${responseId}`, () => {\r",
              "        pm.expect(responseId).to.equal(id);\r",
              "    });\r",
              "\r",
              "    pm.test(`Status ${responseStatus}`, () => {\r",
              "        pm.expect([status, rewriteStatus]).to.include(responseStatus);\r",
              "    });\r",
              "\r",
              "    pm.test(`Version ${responseVersion}`, () => {\r",
              "        pm.expect([version, updateVersion, rewriteVersion]).to.include(responseVersion);\r",
              "    });\r",
              "\r",
              "    pm.test('History array: id, status, version', () => {\r",
              "        pm.expect(responseHistory).to.be.an('array');\r",
              "        responseHistory.forEach((item, index) => {\r",
              "            pm.expect(item, `history[${index}]`).to.have.property('id');\r",
              "            pm.expect(item, `history[${index}]`).to.have.property('status');\r",
              "            pm.expect(item, `history[${index}]`).to.have.property('version');\r",
              "        });\r",
              "    });\r",
              "}"
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        }
      ],
