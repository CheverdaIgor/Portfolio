{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Schema",
  "type": "object",
  "properties": {
    "id": {
      "type": "number"
    },
    "petId": {
      "type": "number"
    },
    "quantity": {
      "type": "number"
    },
    "shipDate": {
      "type": "string"
    },
    "status": {
      "type": "string",
      "enum": ["placed", "approved", "delivered"]
    },
    "complete": {
      "type": "boolean"
    }
  },
  "required": ["shipDate", "status"]
}